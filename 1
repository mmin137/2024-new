# include <stdio.h>
# define SIZE 5

void insertion_sort(int list[], int n){    // 삽입정렬 함수
  int i, j, key;

  for(i=1; i<=n; i++){
    key = list[i];           // 현재 삽입될 i번째 정수를 key 변수로 복사
    j = i-1;
    while(j>=0 && list[j] > key){    // key 값보다 정렬된 배열에 있는 값이 크면
      list[j+1] = list[j];  // j번째를 j+1번째로 이동
      j--;
    }
    list[j+1] = key;
  }
}

int main(void){
  int i;
  int n = SIZE;
  int list[SIZE] = {8, 5, 7, 2, 3};

  insertion_sort(list, n);  // 삽입 정렬 함수 호출

  for(i=0; i<n; i++){    // 정렬 결과 출력
    printf("%d ", list[i]);
  }
}
